{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ApiClient": {
    "HackerAPI": {
      "BaseUrl": "https://hacker-news.firebaseio.com",
      "Authorization": "OAuth",
      "Endpoints": {
        "Stories": "/v0/newstories.json",
        "Detail": "/v0/item/{newsId}.json?print=pretty"
      }
    }
  },
  "Redis": "localhost:6379",
  "AllowedHosts": "*"
}
